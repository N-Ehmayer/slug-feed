  "license": "ISC",
  "devDependencies": {
    "concurrently": "^3.5.1",
    "eslint": "^4.18.2",
    "js-beautify": "^1.7.5"
  },
  "dependencies": {
    "query-string": "^5.1.1"
  }
}
